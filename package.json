        "connect-geschichte-dom": "implicit-dom-connect /home/lenny/thingies/salmutterklaus-website/sources/page-heads/geschichte/document-head.pug 'sources/source/*/*.coffee' /home/lenny/thingies/salmutterklaus-website/sources/source/domconnect/geschichtedomconnect.coffee",
